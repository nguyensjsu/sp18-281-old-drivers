        // Autocomplete funciton
        script.
            $( function() {
                var availableTags = [
                    "San Diego",
                    "Los Angeles",
                    "San Francisco",
                    "Seattle",
                    "Golden Gate Bridge",
                    "Lake Tahoe",
                    "Yosemite National Park",
                    "Napa Valley",
                    "Alcatraz",
                    "Mendocino Coast",
                    "Universal Studios Hollywood",
                    "Griffith Observatory",
                    "Disneyland Park",
                    "Six Flags Magic Mountain",
                    "Pike Place Market",
                    "Chihuly Garden and Glass",
                    "Space Needle",
                    "Washington State Ferries"
                ];
                $( "#tags" ).autocomplete({
                    source: availableTags
                });
            } );
            $( function() {
                $( "#datepicker" ).datepicker({
                    minDate : 0
                });
            } );
